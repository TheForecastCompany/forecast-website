{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },

    { "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|css|js|woff2?|ttf|eot))", "dest": "/$1" },

    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
